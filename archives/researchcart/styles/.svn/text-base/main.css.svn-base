/* table design begins */
table.tbl {
	border-collapse: collapse;
	empty-cells: show;
	border: 1px solid #ccc;
	border-spacing: 5px;
	width: 50%;
}
.tableFont {
	font-family: "Calibri", Arial, sans-serif;
	font-size: 11pt;
	text-align: center;
}
table.tbl td, table.tbl th {
	padding: 3px 8px;
	border: 1px solid #ccc;
}
table.tbl tr {
	background: #f2f2f2;
}
table.tbl th {
	background: #ddd;
}
.data {
	text-align: left;
	font-family: "Calibri", Arial, sans-serif;
	font-size: 11pt;
}
.tblselectall td {
	font-size: 11pt;
	font-weight: bold;
	background: #ddd;
}
h1.heading {
	font-size: 20pt;
	font-weight: bold;
	margin-top: 0px;
	margin-bottom: 10pt;
	text-align: center;
	font-family: "Calibri", Arial, sans-serif;
	width: 100%;
}
h1.headingNote {
	font-size: 20pt;
	font-weight: bold;
	margin-top: 0px;
	margin-bottom: 30pt;
	text-align: center;
	font-family: "Calibri", Arial, sans-serif;
	width: 100%;
}
h2.heading {
	font-size: 20pt;
	font-weight: bold;
	margin-top: 0px;
	margin-bottom: 10pt;
	text-align: center;
	font-family: "Calibri", Arial, sans-serif;
	width: 100%;
	font-style: italic;
}
